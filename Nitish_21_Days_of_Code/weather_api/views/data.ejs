<%- include("header") -%>

<div id="subContainer">
    <h2>Weather Forecast</h2>

    <div>
        <div class="city_name">
            <h3>City :- <%= cityName %></h3>
       </div>
   
       <div class="description">
           <p>Temperature :- <%= Temperature %></p>
           <p>Description :- <%= Description %></p>
       </div>
       
       <div class="image">
           <img src="<%= Image %>" alt="Current climate image" >
       </div>
    </div>
    
</div>

<%- include("footer") -%>